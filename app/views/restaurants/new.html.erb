<h1>Create New Restaurants</h1>
<%= simple_form_for(@restaurant) do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :phone_number %>
  <%= f.input :category, collection: ["chinese", "italian", "japanese", "french", "belgian"], include_blank: false %>
  <%= f.submit "CREATE" %>
<% end %>
